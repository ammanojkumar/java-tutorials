package io.mk.creational.designpattern.mobile.abstractfactoty;

import io.mk.creational.designpattern.os.factoty.MobileOS;
import io.mk.creational.designpattern.os.factoty.OperatingSystem;
import io.mk.creational.designpattern.processor.factory.MobileProcessor;
import io.mk.creational.designpattern.processor.factory.Processor;

public interface MobileAbstractFactory {

	public abstract MobileOS getMobileOS(OperatingSystem operatingSystem);

	public abstract MobileProcessor getMobileProcessor(Processor processor);
}
