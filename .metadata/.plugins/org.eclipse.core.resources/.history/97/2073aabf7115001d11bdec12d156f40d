package io.mk.creational.designpattern.os.factoty;

public class MobileOSFactory {

	public MobileOS getMobileOS(OperatingSystem operatingSystem) {
		switch (operatingSystem) {
		case ANDROID:
			return new OSAndroid();
		case WINDOWS:
			return new OSWindows();
		case MAC:
			return new MAC();
		}
		return null;
	}
}
