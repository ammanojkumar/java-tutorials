package io.mk.creational.designpattern.mobile.abstractfactoty;

import io.mk.creational.designpattern.os.factoty.MobileOS;
import io.mk.creational.designpattern.os.factoty.OSEnum;
import io.mk.creational.designpattern.processor.factory.MobileProcessor;
import io.mk.creational.designpattern.processor.factory.ProcessorBionic;
import io.mk.creational.designpattern.processor.factory.ProcessorEnum;
import io.mk.creational.designpattern.processor.factory.ProcessorMediatek;
import io.mk.creational.designpattern.processor.factory.ProcessorSnapdragon;

public class MobileProcessorFactory implements MobileAbstractFactory {

	@Override
	public MobileOS getMobileOS(OSEnum operatingSystem) {
		return null;
	}

	@Override
	public MobileProcessor getMobileProcessor(ProcessorEnum processor) {
		switch (processor) {
		case SNAPDRAGON:
			return new ProcessorSnapdragon();
		case MEDIATEK:
			return new ProcessorMediatek();
		case BIONIC:
			return new ProcessorBionic();
		}
		return null;
	}
}
